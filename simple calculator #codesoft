a=int(input("enter the first number:"))
b=int(input("enter the second number:"))
operation=input("add/sub/mul/div")
if operation=="add":
    print(a+b)
elif operation=="sub":
    print(a-b)
elif operation=="mul":
    print(a*b)
elif operation=="div":
    print(a/b)
else:
    print("invalid operation")

